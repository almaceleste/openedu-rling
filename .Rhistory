library(tidyverse)
x <- read_csv("./data.japanese.vocal.tract.length.csv")
x %>%
ggplot(aes(x = ID, y = c(a.vtl, e.vtl, i.vtl, o.vtl, u.vtl)))
x %>%
ggplot(aes(x = ID))
x %>%
mutate(a = a.vtl, e = e.vtl, i = i.vtl, o = o.vtl, u = u.vtl) %>%
gather(key = length, value = value, a:u) %>%
ggplot(aes(x = ID, y = value, label = length)) +
# geom_point() +
geom_text() +
labs(
x = 'speakers',
y = 'length of the vocal tract',
caption = 'data from [Hatano et.al 2012]'
)
x %>%
mutate(a = a.vtl, e = e.vtl, i = i.vtl, o = o.vtl, u = u.vtl) %>%
