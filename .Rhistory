library(tidyverse)
library(repr)
library(ggplot2)
x %>%
mutate(a = a.vtl, e = e.vtl, i = i.vtl, o = o.vtl, u = u.vtl) %>%
gather(key = vowel, value = length, a:u) -> tmp
options(repr.plot.width = 16, repr.plot.height = 9)
ggplot(tmp, aes(x = vowel, y = length)) +
# geom_point() +
geom_boxplot() +
geom_hline(aes(yintercept = mean(mean)),linetype = 2) +
# coord_fixed(ratio = 1.58) +
labs(
y = 'length of the vocal tract',
caption = 'data from [Hatano et.al 2012]'
)
